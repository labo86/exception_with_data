{
  "name": "labo86/exception_with_data",
  "description": "Una clase Exception que soporta un array como data",
  "authors": [
    {
      "name": "Edwin Rodríguez-León",
      "email": "edwrodrig@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
